{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Maxim Mazurok",
    "email": "maxim@mazurok.com",
    "url": "https://maxim.mazurok.com"
  },
  "type": "module",
  "scripts": {
    "apis-sync-helper": "npm run ts-node-tool -- bin/apis-sync-helper.ts",
    "auto-publish": "npm run ts-node-tool -- bin/auto-publish/index.ts",
    "precheck": "prettier --check .",
    "check": "gts check",
    "clean": "gts clean",
    "cspell": "npm run ts-node-tool -- bin/cspell.ts",
    "dtslint": "dtslint",
    "prefix": "prettier --write .",
    "fix": "gts fix",
    "get-latest-types": "npm run ts-node-tool -- bin/get-latest-types.ts ./types",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "npm run ts-node-tool -- bin/lint.ts ./types",
    "ncu": "npx -y npm-check-updates -u --doctor",
    "sort": "npx -y sort-package-json",
    "start": "npm run ts-node-tool -- src/cli.ts --out ./types",
    "start-dt": "npx -y rimraf ~/DefinitelyTyped/types/gapi.client.* && npm run ts-node-tool -- src/dt/cli.ts --out ~/DefinitelyTyped/types",
    "pretest": "npm run type-check && npm run cspell",
    "test": "npm run test:unit && npm run test:integration",
    "posttest": "npm run check",
    "test:integration": "npm run jest -- --testRegex '/test/.*\\.integration\\.spec\\.ts$'",
    "test:integration:future": "npm run jest -- --config jest.integration.config.ts",
    "test:unit": "npm run jest",
    "ts-node-tool": "node --loader ts-node/esm",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.7.0",
    "@jest/globals": "^29.5.0",
    "@octokit/rest": "^19.0.5",
    "@types/dedent": "^0.7.0",
    "@types/dot": "^1.1.5",
    "@types/gapi.client.discovery": "^1.0.9",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/n-readlines": "^1.0.3",
    "@types/node": "^18.15.13",
    "@types/request": "2.48.8",
    "@types/sinon": "^10.0.13",
    "@types/validate-npm-package-name": "^4.0.0",
    "commander": "^10.0.0",
    "dedent": "^0.7.0",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "dtslint": "^4.2.1",
    "eslint-plugin-deprecation": "^1.4.1",
    "get-proxy-settings": "^0.1.13",
    "got": "^12.6.0",
    "gts": "^3.1.1",
    "hpagent": "^1.2.0",
    "jest": "^29.5.0",
    "latest-version": "^7.0.0",
    "lodash": "^4.17.21",
    "n-readlines": "^1.0.1",
    "nock": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-packagejson": "^2.4.3",
    "semver": "^7.5.0",
    "sinon": "^15.0.3",
    "strip-json-comments": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "engines": {
    "node": "18.2.0",
    "npm": "8.9.0"
  },
  "scriptComments": {
    "test:integration:future": "TODO: use this script in the future, when jest can use moduleResolution: node16"
  }
}
