{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Maxim Mazurok",
    "email": "maxim@mazurok.com",
    "url": "https://maxim.mazurok.com"
  },
  "scripts": {
    "auto-publish": "node --loader ts-node/esm bin/auto-publish/index.ts",
    "precheck": "prettier --ignore-path ./.gitignore --check .",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "dtslint": "dtslint",
    "prefix": "prettier --ignore-path ./.gitignore --write .",
    "fix": "gts fix",
    "get-latest-types": "node --loader ts-node/esm bin/get-latest-types.ts ./types",
    "lint": "node --loader ts-node/esm bin/lint.ts ./types",
    "ncu": "npx -y npm-check-updates -u --doctor",
    "prepare": "npm run compile",
    "sort": "npx -y sort-package-json",
    "start": "node --loader ts-node/esm src/cli.ts --out ./types",
    "pretest": "npm run compile",
    "test": "mocha",
    "posttest": "npm run check",
    "start-dt": "npx -y rimraf ~/DefinitelyTyped/types/gapi.client.* && node --loader ts-node/esm src/dt/cli.ts --out ~/DefinitelyTyped/types"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@octokit/rest": "^18.9.1",
    "@types/dot": "^1.1.4",
    "@types/gapi.client.discovery": "^1.0.7",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/n-readlines": "^1.0.2",
    "@types/node": "^16.7.4",
    "@types/request": "2.48.7",
    "commander": "^8.1.0",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "dtslint": "^4.1.6",
    "eslint-plugin-deprecation": "^1.2.1",
    "get-proxy-settings": "^0.1.13",
    "got": "^11.8.2",
    "gts": "^3.1.0",
    "hpagent": "^0.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.0",
    "n-readlines": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "strip-json-comments": "^4.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "16.7.0",
    "npm": "7.20.6"
  },
  "type": "module"
}
