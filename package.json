{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Maxim Mazurok",
    "email": "maxim@mazurok.com",
    "url": "https://maxim.mazurok.com"
  },
  "type": "module",
  "scripts": {
    "apis-sync-helper": "node --loader ts-node/esm bin/apis-sync-helper.ts",
    "auto-publish": "node --loader ts-node/esm bin/auto-publish/index.ts",
    "precheck": "prettier --check .",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "cspell": "node --loader ts-node/esm bin/cspell.ts",
    "dtslint": "dtslint",
    "prefix": "prettier --write .",
    "fix": "gts fix",
    "get-latest-types": "node --loader ts-node/esm bin/get-latest-types.ts ./types",
    "lint": "node --loader ts-node/esm bin/lint.ts ./types",
    "ncu": "npx -y npm-check-updates -u --doctor",
    "prepare": "npm run compile",
    "sort": "npx -y sort-package-json",
    "start": "node --loader ts-node/esm src/cli.ts --out ./types",
    "start-dt": "npx -y rimraf ~/DefinitelyTyped/types/gapi.client.* && node --loader ts-node/esm src/dt/cli.ts --out ~/DefinitelyTyped/types",
    "pretest": "npm run compile && npm run cspell",
    "test": "npm run test:unit && npm run test:integration",
    "posttest": "npm run check",
    "postinstall": "patch-package",
    "test:integration": "mocha test/**/*.integration.spec.ts",
    "test:unit": "mocha 'test/**/!(*.integration).spec.ts'"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.7.0",
    "@octokit/rest": "^19.0.4",
    "@types/chai": "^4.3.3",
    "@types/dedent": "^0.7.0",
    "@types/dot": "^1.1.5",
    "@types/gapi.client.discovery": "^1.0.7",
    "@types/lodash": "^4.14.184",
    "@types/mocha": "^9.1.1",
    "@types/n-readlines": "^1.0.3",
    "@types/node": "^18.7.18",
    "@types/request": "2.48.8",
    "@types/sinon": "^10.0.13",
    "@types/validate-npm-package-name": "^4.0.0",
    "chai": "^4.3.6",
    "commander": "^9.4.0",
    "dedent": "^0.7.0",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "dtslint": "^4.2.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "get-proxy-settings": "^0.1.13",
    "got": "^12.4.1",
    "gts": "^3.1.1",
    "hpagent": "^1.0.0",
    "jest-resolve": "^29.0.2",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "n-readlines": "^1.0.1",
    "nock": "^13.2.9",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "sinon": "^14.0.0",
    "strip-json-comments": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "validate-npm-package-name": "^4.0.0"
  },
  "dependenciesComments": {
    "jest-resolve": "Only needed to fix TS error for @jest/test-result: Cannot find module 'jest-resolve' or its corresponding type declarations",
    "jest-snapshot": "Only needed to fix TS error for mocha-chai-jest-snapshot: Cannot find name 'SnapshotStateOptions'; see https://github.com/mochiya98/mocha-chai-jest-snapshot/issues/15"
  },
  "engines": {
    "node": "18.2.0",
    "npm": "8.9.0"
  }
}
