{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "scripts": {
    "auto-update": "ts-node bin/auto-update/index.ts",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "dtslint": "dtslint",
    "fix": "gts fix",
    "get-latest-types": "ts-node bin/get-latest-types.ts ./types",
    "lint": "ts-node bin/lint.ts ./types",
    "prepare": "npm run compile",
    "start": "ts-node src/cli.ts --out ./types",
    "pretest": "npm run compile",
    "test": "mocha",
    "posttest": "npm run check"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@octokit/rest": "^17.9.0",
    "@types/bluebird": "^3.5.30",
    "@types/commander": "^2.12.2",
    "@types/dot": "^1.1.3",
    "@types/gapi.client.discovery": "^1.0.1",
    "@types/line-reader": "0.0.28",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.38",
    "@types/request": "2.48.4",
    "bluebird": "^3.7.2",
    "commander": "^5.1.0",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "dtslint": "^3.5.2",
    "gts": "^2.0.2",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "parse-git-status": "^0.1.0",
    "request": "^2.88.2",
    "ts-node": "^8.10.1",
    "typescript": "3.9.0-dev.20200330"
  },
  "engines": {
    "npm": "6.14.4"
  }
}
