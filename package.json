{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Maxim Mazurok",
    "email": "maxim@mazurok.com",
    "url": "https://maxim.mazurok.com"
  },
  "type": "commonjs",
  "scripts": {
    "apis-sync-helper": "npm run ts-node-tool -- bin/apis-sync-helper.ts",
    "auto-publish": "npm run ts-node-tool -- bin/auto-publish/index.ts",
    "precheck": "prettier --check .",
    "check": "gts check",
    "clean": "gts clean",
    "cspell": "npm run ts-node-tool -- bin/cspell.ts",
    "dtslint": "node node_modules/@definitelytyped/dtslint/",
    "prefix": "prettier --write .",
    "fix": "gts fix && npm run sort",
    "postinstall": "patch-package",
    "lint": "npm run ts-node-tool -- bin/lint.ts ./types",
    "ncu": "npx -y npm-check-updates -u --doctor",
    "start": "npm run ts-node-tool -- src/cli.ts --out ./types",
    "poststart": "prettier --ignore-path --write types/**/*",
    "start-dt": "npx -y rimraf ~/DefinitelyTyped/types/gapi.client.* && npm run ts-node-tool -- src/dt/cli.ts --out ~/DefinitelyTyped/types",
    "pretest": "npm run type-check && npm run cspell",
    "test": "vitest --run",
    "posttest": "npm run check",
    "ts-node-tool": "ts-node",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@definitelytyped/dtslint": "0.2.10",
    "@definitelytyped/eslint-plugin": "0.1.4",
    "@expo/spawn-async": "^1.7.2",
    "@octokit/rest": "^20.0.2",
    "@types/dedent": "^0.7.2",
    "@types/dot": "^1.1.7",
    "@types/gapi.client.discovery-v1": "^0.0.4",
    "@types/lodash": "^4.14.202",
    "@types/n-readlines": "^1.0.6",
    "@types/node": "^20.11.19",
    "@types/validate-npm-package-name": "^4.0.2",
    "commander": "^12.0.0",
    "dedent": "^1.5.1",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "get-port": "^7.0.0",
    "get-proxy-settings": "^0.1.13",
    "got": "^14.2.0",
    "gts": "^5.2.0",
    "hpagent": "^1.2.0",
    "latest-version": "^8.0.0",
    "lodash": "^4.17.21",
    "n-readlines": "^1.0.1",
    "nock": "^13.5.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "prettier-plugin-packagejson": "^2.4.11",
    "semver": "^7.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "validate-npm-package-name": "^5.0.0",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "dependencyComments": {
    "@definitelytyped/dtslint": "relies on @definitelytyped/eslint-plugin, update them together",
    "@definitelytyped/eslint-plugin": "needed for @definitelytyped/dtslint, update them together"
  }
}
