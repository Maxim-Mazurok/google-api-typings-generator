{
  "name": "google-api-typings-generator",
  "description": "Google API Typescript typings definitions generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxim-Mazurok/google-api-typings-generator.git"
  },
  "license": "MIT",
  "scripts": {
    "auto-update": "ts-node bin/auto-update/index.ts",
    "precheck": "prettier --ignore-path ./.gitignore --check .",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "dtslint": "dtslint",
    "prefix": "prettier --ignore-path ./.gitignore --write .",
    "fix": "gts fix",
    "get-latest-types": "ts-node bin/get-latest-types.ts ./types",
    "lint": "ts-node bin/lint.ts ./types",
    "ncu": "npx npm-check-updates",
    "prepare": "npm run compile",
    "sort": "npx sort-package-json",
    "start": "ts-node src/cli.ts --out ./types",
    "pretest": "npm run compile",
    "test": "mocha",
    "posttest": "npm run check"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@octokit/rest": "^18.0.4",
    "@types/commander": "^2.12.2",
    "@types/dot": "^1.1.4",
    "@types/gapi.client.discovery": "^1.0.4",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/n-readlines": "^1.0.1",
    "@types/node": "^12.12.55",
    "@types/request": "2.48.5",
    "commander": "^6.1.0",
    "deep-sort-object": "^1.0.2",
    "dot": "^1.1.3",
    "dtslint": "latest",
    "get-proxy-settings": "^0.1.12",
    "gts": "github:google/gts#master",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "n-readlines": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "parse-git-status": "^0.1.0",
    "request": "^2.88.2",
    "strip-json-comments": "^3.1.1",
    "ts-node": "^9.0.0",
    "typescript": "latest"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  }
}
